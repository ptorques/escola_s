programa
{
	funcao inicio()
	{
		real num1, num2
		escreva("Insira o primeiro número: ")
		leia (num1)
		escreva("Insira o segundo número: ")
		leia (num2)
		se (num1 + num2 > 20)
		{
			escreva("\nSoma: " + (num1 + num2 + 8))
		}
		senao {
			escreva("\nSoma: " + (num1 + num2 - 5))
		}
	}
}
