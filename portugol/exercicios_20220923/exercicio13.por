programa
{
	inclua biblioteca Matematica
	
	funcao inicio()
	{
		real num1
		escreva("\nInsira o número: ")
		leia(num1)
		escreva("\nLogaritmo: " + Matematica.logaritmo(num1, 10.0))
	}
}
